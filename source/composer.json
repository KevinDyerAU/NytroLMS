{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "arrilot/laravel-widgets": "^3.13",
        "barryvdh/laravel-dompdf": "^3.1",
        "doctrine/dbal": "^3.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/slack-notification-channel": "^3.5",
        "laravel/telescope": "^5.7",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "microsoft/microsoft-graph": "*",
        "opcodesio/log-viewer": "*",
        "pragmarx/countries": "^0.8",
        "predis/predis": "^2.3",
        "rap2hpoutre/fast-excel": "^5.6",
        "sentry/sentry-laravel": "*",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-permission": "^6.0",
        "unisharp/laravel-filemanager": "^2.10",
        "van-ons/laraberg": "^1.1",
        "yajra/laravel-datatables": "^11",
        "yajra/laravel-datatables-html": "~11",
        "yajra/laravel-datatables-oracle": "~11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.27",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "theanik/laravel-more-command": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
